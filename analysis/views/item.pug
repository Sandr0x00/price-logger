extends layout.pug

block content
  div.container
    div.row
      each item in items
        div.col-6.col-sm-4.col-md-3
          a.items(href="#", data-id=item)= item
    h1
      a(href=link, target="_blank")= id
    div.row
      if img
        div.col-md-2
          img(src=img)
      div#price-chart.col-md-10(data-id=id)
